        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Query01-limit100" enabled="true">
          <stringProp name="dataSource">Pool1</stringProp>
          <stringProp name="query">SELECT vendor_id, pickup_date, pickup_datetime, dropoff_date, dropoff_datetime, passenger_count, trip_distance_mi, pickup_longitude, pickup_latitude, rate_code, store_and_fwd_flag, dropoff_longitude, dropoff_latitude, payment_type, fare_amount, surcharge, mta_tax, tip_amount, tolls_amount, total_amount
FROM Business.Transportation.&quot;NYC Trips&quot;
WHERE (&apos;2011-09-01 00:00:00&apos; &lt;= pickup_datetime AND &apos;2012-03-01 00:00:00&apos; &gt; pickup_datetime) AND (10.47600000000001 &lt;= trip_distance_mi)
limit 100</stringProp>
